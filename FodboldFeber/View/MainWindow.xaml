<Window x:Class="FodboldFeber.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FodboldFeber"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Grid Margin="0,0,-0.4,-1" Height="421" VerticalAlignment="Top">
        <Button x:Name="Frontpage" Content="Forside" HorizontalAlignment="Left" Margin="45,10,0,0" VerticalAlignment="Top" Width="75" Foreground="Red" Background="White" BorderThickness="0" FontWeight="Bold" Click="Frontpage_Click"/>
        <Button x:Name="News" Content="Nyheder" HorizontalAlignment="Left" Margin="148,10,0,0" VerticalAlignment="Top" Width="75" Foreground="Red" Background="White" BorderThickness="0" FontWeight="Bold" Click="News_Click"/>
        <Button x:Name="Shop" Content="Shop" HorizontalAlignment="Left" Margin="263,10,0,0" VerticalAlignment="Top" Width="75" Foreground="Red" Background="White" BorderThickness="0" FontWeight="Bold" Click="Shop_Click"/>
        <Button x:Name="Contact" Content="Kontakt" HorizontalAlignment="Left" Margin="467,10,0,0" VerticalAlignment="Top" Width="75" Foreground="Red" Background="White" BorderThickness="0" FontWeight="Bold" Click="Contact_Click"/>
        <Button x:Name="About" Content="Om os" HorizontalAlignment="Left" Margin="547,10,0,0" VerticalAlignment="Top" Width="75" Foreground="Red" Background="White" BorderThickness="0" FontWeight="Bold" Click="About_Click"/>
        <Button x:Name="Customers" Content="Kunder" HorizontalAlignment="Left" Margin="634,10,0,0" VerticalAlignment="Top" Width="75" Foreground="Red" Background="White" BorderThickness="0" FontWeight="Bold" Click="Customers_Click"/>
        <Button x:Name="Login" Content="Login" HorizontalAlignment="Left" Margin="709,10,0,0" VerticalAlignment="Top" Width="75" Foreground="Red" Background="White" BorderThickness="0" FontWeight="Bold" Click="Login_Click"/>
        <Frame x:Name="StartPage" Margin="0,40,0,-147" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
